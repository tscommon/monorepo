{
  "name": "@tscommon/monorepo",
  "description": "A set of common TypeScript constructs",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "format": "prettier . --check",
    "lint": "eslint",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "ci:publish": "pnpm publish -r --access public && changeset tag",
    "ci:version": "pnpm changeset version && pnpm install --no-frozen-lockfile"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.10",
    "@eslint/js": "9.15.0",
    "@vitest/coverage-v8": "2.1.5",
    "docusaurus-plugin-typedoc-api": "4.4.0",
    "eslint": "9.15.0",
    "globals": "15.12.0",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "tsd": "0.31.2",
    "typedoc": "0.26.11",
    "typescript": "5.7.2",
    "typescript-eslint": "8.15.0",
    "vitest": "2.1.5"
  }
}
